@model WebApplicationPruebaDesarrolloCliente.Models.Transacciones

@{
    ViewData["Title"] = "BusquedaPorFecha";
}

<div class="text-center">
    <h1 class="display-4">Bienvenido</h1>

</div>

<h2>Busqueda de Transacciones Transacciones</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="PasoBusquedaPorFecha">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FechaTransaccion" class="control-label"></label>
                <input asp-for="FechaTransaccion" class="form-control" />
                <span asp-validation-for="FechaTransaccion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Buscar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
